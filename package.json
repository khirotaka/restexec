{
  "name": "restexec",
  "version": "1.0.0",
  "description": "REST API for executing TypeScript code",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "node --watch dist/index.js",
    "dev:build": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "typescript",
    "code-execution",
    "rest-api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "@types/supertest": "^6.0.3",
    "@vitest/ui": "^4.0.8",
    "supertest": "^7.1.4",
    "typescript": "^5.3.3",
    "vitest": "^4.0.8"
  }
}
